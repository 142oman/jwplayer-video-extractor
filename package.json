{
  "name": "jwplayer-video-extractor",
  "version": "1.0.0",
  "description": "Extract video sources from JW Player pages",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs jwplayer-extractor",
    "pm2:monit": "pm2 monit",
    "build": "echo 'No build step required'",
    "example": "node examples/api-example.js",
    "setup:ubuntu": "bash scripts/setup-ubuntu.sh",
    "deploy:ubuntu": "bash scripts/deploy-ubuntu.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.6.1",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}